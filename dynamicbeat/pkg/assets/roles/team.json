{
  "elasticsearch": {
    "indices": [
      {
        "names": [
          "results-{{.Team}}"
        ],
        "privileges": [
          "read"
        ]
      },
      {
        "names": [
          "attributes-{{.Team}}"
        ],
        "privileges": [
          "read",
          "view_index_metadata"
        ]
      },
      {
        "names": [
          "attribute-values-{{.Team}}"
        ],
        "privileges": [
          "read",
          "index",
          "view_index_metadata"
        ]
      }
    ]
  }
}